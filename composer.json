{
    "name": "survos/barcode-bundle",
    "description": "Integrates picqer/php-barcode-generator in a Symfony application",
    "type": "symfony-bundle",
    "keywords": ["barcode", "symfony"],
    "require": {
        "php": "^8.1",
        "picqer/php-barcode-generator": "^2.2",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/http-kernel": "^6.1",
        "twig/twig": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Survos\\BarcodeBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.7"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "id": "barcode-bundle.survos.com"
        }
    }

}
